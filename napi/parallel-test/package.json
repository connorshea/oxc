{
  "name": "parallel-test",
  "version": "0.1.0",
  "main": "src-js/index.js",
  "type": "module",
  "scripts": {
    "build": "pnpm run build-dev --release",
    "build-dev": "napi build --platform --js ./bindings.js --dts ./bindings.d.ts --output-dir src-js --no-dts-cache --esm",
    "start": "node src-js/direct/index.js",
    "test": "vitest --dir ./test run"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "description": "Test parallel workers",
  "author": "Boshen and oxc contributors",
  "license": "MIT",
  "homepage": "https://oxc.rs",
  "bugs": "https://github.com/oxc-project/oxc/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/oxc-project/oxc.git",
    "directory": "napi/parallel-test"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "src-js"
  ],
  "devDependencies": {
    "vitest": "catalog:"
  },
  "napi": {
    "binaryName": "parallel-test",
    "targets": [
      "win32-x64",
      "win32-arm64",
      "linux-x64-gnu",
      "linux-arm64-gnu",
      "linux-x64-musl",
      "linux-arm64-musl",
      "darwin-x64",
      "darwin-arm64"
    ]
  }
}
